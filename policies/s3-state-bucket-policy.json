{
    "Version": "2012-10-17",
    "Statement": [
        {
            "Sid": "AllowOrganizationAccountAccessRole",
            "Effect": "Allow",
            "Principal": {
                "AWS": "arn:aws:iam::ACCOUNT_ID:role/OrganizationAccountAccessRole"
            },
            "Action": [
                "s3:ListBucket",
                "s3:GetObject",
                "s3:PutObject",
                "s3:DeleteObject"
            ],
            "Resource": [
                "arn:aws:s3:::static-website-state-ENV",
                "arn:aws:s3:::static-website-state-ENV/*"
            ]
        }
    ]
}